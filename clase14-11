    secondlast_name=''
    ## fin atributos
    ##metodos
    def __init__(self,id,run,dv,alternative_id,names,firstlast_name,secondlast_name):
        self.id = id ###TODO LO KE SETEE
        self.run = run
        self.dv = dv
        self.alternative_id = alternative_id
        self.names = names
        self.firstlas_name = firstlas_name
        self.secondlast_name = secondlast_name
    ##entrada de datas
